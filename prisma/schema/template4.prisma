model Template4 {
  id                    String   @id @default(cuid())
  nameEn                String?
  nameAr                String?
  seoDescriptionEn      String?
  seoDescriptionAr      String?
  isActive              Boolean  @default(true)
  description1En        String?  @db.Text
  description1Ar        String?  @db.Text
  description2En        String?  @db.Text
  description2Ar        String?  @db.Text
  description3En        String?  @db.Text
  description3Ar        String?  @db.Text
  description4En        String?  @db.Text
  description4Ar        String?  @db.Text
  image1                String?
  image2                String?
  image3                String?
  buttonText1En         String?
  buttonText1Ar         String?
  buttonText2En         String?
  buttonText2Ar         String?
  buttonNavigation1En   String?
  buttonNavigation1Ar   String?
  buttonNavigation2En   String?
  buttonNavigation2Ar   String?
  page                  Page?    @relation(fields: [pageId], references: [id], onDelete: Cascade)
  pageId                String?
  createdAt             DateTime @default(now())
  updatedAt             DateTime @updatedAt
}
