model Page {
  id        String   @id @default(cuid())
  nameEn    String?
  nameAr    String?
  slug      String?
  template  String?
  subMenus  SubMenu[]
  template1 Template1[]
  template2 Template2[]
  template3 Template3[]
  template4 Template4[]
  slider    Slider[]
  whyBarcode WhyBarcode[]
  coreSolution CoreSolution[]
  proService ProService[]
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}