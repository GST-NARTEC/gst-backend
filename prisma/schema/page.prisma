model Page {
  id                                String         @id @default(cuid())
  nameEn                            String?
  nameAr                            String?
  slug                              String?
  template                          String?
  createdAt                         DateTime       @default(now())
  updatedAt                         DateTime       @updatedAt

  headers                           SmartSolution[]
  coreSolution                      CoreSolution[]
  proService                        ProService[]
  slider                            Slider[]
  subMenus                          SubMenu[]
  template1                         Template1[]
  template2                         Template2[]
  template3                         Template3[]
  template4                         Template4[]
  caseStudyTemplate1                CaseStudyTemplate1[]
  whyBarcode                        WhyBarcode[]
