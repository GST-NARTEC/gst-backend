model ProService {
    id              String  @id @default(uuid())
    image           String?
    titleEn         String?
    titleAr         String?
    descriptionEn   String?
    descriptionAr   String?
    captionEn       String?
    captionAr       String?
    status          Int     @default(1)
    createdAt       DateTime @default(now())
    updatedAt       DateTime @updatedAt
}