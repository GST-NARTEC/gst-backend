model GTIN {
  id            String         @id @default(uuid()) @db.UniqueIdentifier
  gtin          String?        @unique
  status        String?        @default("Available")
  usageStatus   String?        @default("Unused") 
  assignedGtins AssignedGtin[]
  createdAt     DateTime       @default(now())
  updatedAt     DateTime       @updatedAt
}
