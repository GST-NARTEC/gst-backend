model Aggregation {
  id                    Int @id @default(autoincrement())
  serialNo              String?
  qty                   Int?
  batchNo               String?
  expiryDate            DateTime? @default(now())
  manufacturingDate     DateTime? @default(now())
  gtin                  String?
  userId                String?
  createdAt             DateTime @default(now())
  updatedAt             DateTime @updatedAt
}
